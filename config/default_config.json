    "val_size": 0.1,
    "random_seed": 42
  },
  "audio_processing": {
    "sample_rate": 44100,
    "n_mels": 128,
    "n_fft": 2048,
    "hop_length": 512,
    "duration": 5.0
  },
  "paths": {
    "raw_data": "data/raw",
    "processed_data": "data/processed",
    "models": "models",
    "checkpoints": "checkpoints",
    "logs": "logs"
  }
}
